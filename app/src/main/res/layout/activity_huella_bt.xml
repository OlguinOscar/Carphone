        android:fontFamily="@font/chau_philomene_one"
        android:text="Escanee su huella para encender el Automovil"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/BT"
        android:layout_width="68dp"
        android:layout_height="69dp"
        android:layout_marginStart="74dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="296dp"
        android:background="@android:drawable/stat_sys_data_bluetooth"
        android:backgroundTint="@color/material_dynamic_primary10"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/noconectado"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/stat_sys_data_bluetooth" />

    <ImageButton
        android:id="@+id/Huella"
        android:layout_width="94dp"
        android:layout_height="103dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="213dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/fingerprint_dialog_fp_icon"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:srcCompat="@drawable/fingerprint_dialog_fp_icon" />

    <TextView
        android:id="@+id/notexto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="259dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="235dp"
        android:fontFamily="@font/chau_philomene_one"
        android:text="No Conectado"
